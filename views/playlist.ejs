<%- include('partials/header') %>
<%- include('partials/navbar') %>  

<div class="container mt-4">
  <div class="row">
    <div class="col-lg-12">
      <h3 class="text-center">My Playlist</h3>
    </div>
  </div>
  <div class="row">
    <% playlist.forEach(row => { %>
      <div class="col-md-3 col-sm-12 d-flex justify-content-center align-items-center">
        <a href="/playlist_detail/<%= row.id %> ">
          <img class="my-3" width="300" src="<%= row.images[0].url %> " alt="">
        </a>
      </div>
    <% }) %>
</div>

<%- include('partials/footer') %>
